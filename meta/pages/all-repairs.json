                    "click": [
                      {
                        "path": "add-repairs",
                        "then": [
                          {
                            "code": "await grid0.Reload();",
                            "type": "execute"
                          },
                          {
                            "code": "await InvokeAsync(() => { StateHasChanged(); });",
                            "type": "execute"
                          }
                        ],
                        "type": "popup"
                      }
                    ]
                  },
                  "icon": "add_circle_outline",
                  "name": "button0",
                  "style": {
                    "marginBottom": "10px"
                  },
                  "text": "Dodaj",
                  "type": "button"
                },
                {
                  "dateFormat": "dd.MM.yyyy",
                  "events": {
                    "change": [
                      {
                        "name": "GetRepairs",
                        "schema": {
                          "async": true,
                          "dotnetType": "System.Threading.Tasks.Task<System.Linq.IQueryable<SimpleCarRepair.Models.SimpleCarRepairDb.Repair>>",
                          "items": {
                            "$ref": "#/definitions/Repair",
                            "dotnetType": "SimpleCarRepair.Models.SimpleCarRepairDb.Repair"
                          },
                          "type": "array"
                        },
                        "then": [
                          {
                            "name": "Repairs",
                            "type": "set",
                            "value": "${result}",
                            "valueType": "IEnumerable<SimpleCarRepair.Models.SimpleCarRepairDb.Repair>"
                          }
                        ],
                        "type": "invoke-method"
                      },
                      {
                        "name": "PartReapirs",
                        "type": "set",
                        "value": "new List<PartReapir>()",
                        "valueType": "IEnumerable<SimpleCarRepair.Models.SimpleCarRepairDb.PartReapir>"
                      }
                    ]
                  },
                  "name": "datepicker0",
                  "type": "datepicker",
                  "value": "${Date}"
                }
              ],
              "name": "col0",
              "style": {
                "marginBottom": "10px",
                "paddingBottom": ""
              },
              "type": "col"
            }
          ],
          "name": "row0",
          "type": "row"
        },
        {
          "components": [
            {
              "class": "col-md-12",
              "components": [
                {
                  "allowFiltering": true,
                  "allowPaging": true,
                  "allowSorting": true,
                  "columns": [
                    {
                      "property": "Id",
