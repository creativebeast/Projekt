                    },
                    {
                      "property": "Description",
                      "template": "",
                      "title": "Opis"
                    },
                    {
                      "filterTemplate": "",
                      "filterable": false,
                      "property": "CarImage",
                      "sortable": false,
                      "template": [
                        {
                          "attributes": [
                            {
                              "name": "@onclick:stopPropagation",
                              "value": "true"
                            }
                          ],
                          "events": {
                            "click": [
                              {
                                "parameters": [
                                  {
                                    "name": "id",
                                    "value": "${data.Id}"
                                  }
                                ],
                                "path": "image-reapir-car",
                                "title": "Repair Car",
                                "type": "popup"
                              }
                            ]
                          },
                          "icon": "perm_media",
                          "name": "button1",
                          "size": "ButtonSize.Small",
                          "text": "",
                          "type": "button"
                        }
                      ],
                      "textAlign": "center",
                      "title": "Pojazd",
                      "width": "100px"
                    },
                    {
                      "filterable": false,
                      "property": "DamageImage",
                      "sortable": false,
                      "template": [
                        {
                          "attributes": [
                            {
                              "name": "@onclick:stopPropagation",
                              "value": "true"
                            }
                          ],
                          "buttonStyle": "secondary",
                          "events": {
                            "click": [
                              {
                                "parameters": [
                                  {
                                    "name": "id",
                                    "value": "${data.Id}"
                                  }
                                ],
                                "path": "image-reapir-car-damage",
                                "title": "Car Damage",
                                "type": "popup"
                              }
                            ]
                          },
                          "icon": "insert_photo",
                          "name": "button2",
                          "size": "ButtonSize.Small",
                          "text": "",
                          "type": "button"
                        }
                      ],
                      "textAlign": "center",
                      "title": "Uszkodzenie",
                      "width": "100px"
                    },
                    {
                      "filterTemplate": "",
