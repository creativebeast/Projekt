{
  "access": [
    "Everybody"
  ],
  "components": [
    {
      "components": [
        {
          "name": "pageTitle",
          "size": "H1",
          "text": "Logowanie",
          "type": "heading"
        },
        {
          "components": [
            {
              "class": "col-md-12",
              "components": [
                {
                  "action": "@($\"account/login?redirectUrl={redirectUrl}\")",
                  "components": [
                    {
                      "allowResetPassword": false,
                      "events": {
                        "login": [],
                        "register": [
                          {
                            "path": "register-application-user",
                            "type": "popup"
                          }
                        ]
                      },
                      "loginText": "Zaloguj",
                      "name": "login0",
                      "passwordText": "Haslo",
                      "registerMessageText": "Nie masz jeszcze konta?",
                      "registerText": "Zarejestruj siÄ™",
                      "type": "login",
                      "userText": "Login"
                    }
                  ],
                  "data": "${\"login\"}",
                  "method": "post",
                  "name": "templateForm0",
                  "type": "template-form"
                }
              ],
              "type": "col"
            }
          ],
          "type": "row"
        }
      ],
      "container": "main",
      "type": "content"
    }
  ],
  "events": {
    "load": [
      {
        "code": "var query = System.Web.HttpUtility.ParseQueryString(new Uri(UriHelper.ToAbsoluteUri(UriHelper.Uri).ToString()).Query);",
        "type": "execute"
      },
      {
        "code": "var error = query.Get(\"error\");",
        "type": "execute"
      },
      {
        "name": "redirectUrl",
        "type": "set",
        "value": "query.Get(\"redirectUrl\");"
      },
      {
        "condition": "!string.IsNullOrEmpty(error)",
        "detail": "${error}",
        "severity": "error",
        "summary": "Error",
        "type": "notify"
      }
    ]
  },
  "layout": "Login",
  "name": "Login"
}